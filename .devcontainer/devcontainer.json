{
  "name": "Vue.js Development Container",
  "dockerFile": "Dockerfile",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "github.copilot",
    "github.copilot-chat"
  ],
  "forwardPorts": [
    8080
  ],
  "postCreateCommand": "npm install",
  "remoteUser": "node"
}
